{
  "name": "@sunny-g/cycle-utils",
  "version": "0.1.2",
  "description": "Higher-order components and utilities for Cycle.js",
  "author": "Sunny G <sunny.gonna@gmail.com> (http://blog.sunnyg.io)",
  "homepage": "https://github.com/sunny-g/cycle-utils",
  "repository": "https://github.com/sunny-g/cycle-utils",
  "bugs": "https://github.com/sunny-g/cycle-utils/issues",
  "license": "ISC",
  "main": "commonjs/cycle-utils.js",
  "module": "es2015/index.js",
  "types": "es2015/index.d.ts",
  "files": [
    "commonjs/",
    "es2015/",
    "src/"
  ],
  "keywords": [
    "cycle",
    "utils",
    "rxjs",
    "mostjs",
    "xstream"
  ],
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "prepublish": "tsc && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/*.test.(ts|js)"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "transform": {
      "\\.(ts|js)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "dependencies": {
    "@types/ramda": "0.0.4",
    "ramda": "^0.23.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "ts-jest": "^19.0.8",
    "ts-loader": "^2.0.1",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
